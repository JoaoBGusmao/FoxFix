{
  "manifest_version": 2,
  "name": "FoxFix",
  "version": "0.1",
  "description": "Corrige o preço do Bitcoin no título da FoxBit",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [{
    "js": ["./background.js"],
    "matches": ["https://foxbit.exchange/*"]
  }]
}